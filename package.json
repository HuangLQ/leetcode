    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/tangweikun/long-claw"
  },
  "keywords": [
    "javascript",
    "interview"
  ],
  "author": "tangweikun",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^21.1.9",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^21.2.1",
    "regenerator-runtime": "^0.11.1",
    "ts-jest": "^22.0.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {},
  "CC_TEST_REPORTER_ID": "d6e0a0a758ed9682cdf8dd44833994a02a8c8e220119eeade45cdccb8d9e20ec",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
