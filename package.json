{
  "name": "long-claw",
  "version": "1.0.0",
  "description": "interview questions",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js --presets es2015",
    "watch": "jest --watch",
    "test": "jest --coverage"
  },
